seat seat0 xcursor_theme Oxygen_Black 48
bindsym $mod+F12 exec sh -c '( pgrep copyq || copyq ) && copyq show "&Clipper"'
exec swaykbdd
exec copyq
exec gentoo-pipewire-launcher &
exec swayidle -w \
	timeout 600 'swaymsg "output * dpms off"' \
	  resume 'swaymsg "output * dpms on"'
exec udiskie -Ant &

