bindsym d exec ydotool mousemove -w -- 0 -1
bindsym 0 mode $mouse_1
bindsym 5 mode $mouse50
bindsym 9 mode $mouse99
bindsym plus mode $mouse50
bindsym minus mode $mouse_1
bindsym Control+Right mode $mouse50
